{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./build",
    "baseUrl": ".",
    "paths": {
      "/*": ["./*"],
      "@api/*": ["./api/*"],
      "@models/*": ["./models/*"]
    }
  }
}
